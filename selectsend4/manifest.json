{
  "manifest_version": 3,
  "name": "Tab Audio Player",
  "version": "1.0",
  "permissions": ["tabs"],
  "host_permissions": ["<all_urls>"],
  "action": {},
  "background": {"service_worker": "background.js"},
  "content_scripts": [{
    "matches": ["<all_urls>"],
    "js": ["content.js"],
    "run_at": "document_start"
  }]
}